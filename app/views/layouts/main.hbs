<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>bballdaniel3.com</title>
    <link rel="stylesheet" href="/css/animations.css">
    <link rel="stylesheet" href="/css/blog.css">
    <link rel="stylesheet" href="/css/footer.css">
    <link rel="stylesheet" href="/css/header.css">
    <link rel="stylesheet" href="/css/main.css">
    <link rel="stylesheet" href="/css/guestbook.css">
    <link rel="stylesheet" href="/css/games.css">
    <link rel="stylesheet" href="/css/storyGame/results.css">
    <link rel="stylesheet" href="/css/storyGame/easyMDE.css">
    <link rel="stylesheet" href="/css/projects.css">
    <script>(function () { try { var t = localStorage.getItem('site-theme'); if (!t) { if (window.matchMedia('(prefers-color-scheme: dark)').matches) { t = 'dark'; } } if (t === 'dark') { document.documentElement.classList.add('js'); document.addEventListener('DOMContentLoaded', function () { document.body.classList.add('dark-theme'); }); } } catch (e) { } })();</script>
    <script defer src="https://unpkg.com/swup@4"></script>
    <script defer src="https://unpkg.com/@swup/forms-plugin@3"></script>
    <script defer type="module" src="/swup.js"></script>
    <script defer src="/js/theme.js"></script>
    <script defer src="/js/footer.js"></script>
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/easymde/dist/easymde.min.css" />
    <script src="https://cdn.jsdelivr.net/npm/easymde/dist/easymde.min.js"></script>
    {{#each scripts}}
    <script type="text/javascript" src="{{script}}"></script>
    {{/each}}
</head>

<body class="main-background">
    <header>
        {{> navbar }}
    </header>
    <main id="swup" class="transition-fade">
        <div class="horizontal-margins-lg">
            {{{body}}}
        </div>
    </main>
    <footer>
        {{> footer }}
    </footer>
</body>

</html>